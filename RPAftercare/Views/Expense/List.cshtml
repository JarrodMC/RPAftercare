@model IEnumerable<RPAftercare.Models.tblExpense>

@{
    ViewBag.Title = "List";
}

<h2>Aftercare Expenses</h2>
<div>
    @Html.ActionLink("Add an Expense", "Add")
</div>

<table class="table">
<tr class="label-primary">
    <th>
        @Html.DisplayNameFor(model => model.ExpAmount)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.expDesc)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.expDate)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.expRemarks)
    </th>
    <th></th>
</tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            
            @Html.DisplayFor(modelItem => item.ExpAmount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.expDesc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.expDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.expRemarks)
        </td>
    </tr>
}

</table>